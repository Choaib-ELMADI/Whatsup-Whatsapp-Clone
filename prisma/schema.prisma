generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Status {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  fileUrl   String
  type      String
  userId    String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("stories")
}
